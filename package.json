{
  "name": "ember-pouch",
  "version": "7.0.0",
  "description": "PouchDB adapter for Ember Data",
  "keywords": [
    "ember-addon",
    "Ember",
    "Octane",
    "Data",
    "adapter",
    "PouchDB",
    "CouchDB"
  ],
  "bugs": {
    "url": "https://github.com/pouchdb-community/ember-pouch/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pouchdb-community/ember-pouch.git"
  },
  "license": "Apache-2.0",
  "author": "Nolan Lawson",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "license": "license-checker --summary --failOn GPL",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*",
    "lint:hbs": "ember-template-lint .",
    "lint:js": "eslint .",
    "start": "ember server",
    "test": "npm-run-all lint:* test:*",
    "test:ember": "ember test",
    "test:ember-compatibility": "ember try:each"
  },
  "dependencies": {
    "broccoli-file-creator": "^2.1.1",
    "broccoli-stew": "^2.1.0",
    "ember-cli-babel": "^7.26.10",
    "ember-cli-htmlbars": "^5.7.2",
    "pouchdb-browser": "^7.1.1",
    "pouchdb-find": "^7.1.1",
    "relational-pouch": "^4.0.0"
  },
  "devDependencies": {
    "@ember/optional-features": "^1.3.0",
    "@glimmer/component": "^1.0.0",
    "@glimmer/tracking": "^1.0.0",
    "babel-eslint": "^10.1.0",
    "broccoli-asset-rev": "^3.0.0",
    "ember-auto-import": "~2.4.1",
    "ember-cli": "~4.3.0",
    "ember-cli-app-version": "~5.0.0",
    "ember-cli-dependency-checker": "^3.2.0",
    "ember-cli-inject-live-reload": "^2.1.0",
    "ember-cli-release": "^0.2.9",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-uglify": "^3.0.0",
    "ember-data": "~4.1.0",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-export-application-global": "^2.0.1",
    "ember-inflector": "^4.0.2",
    "ember-load-initializers": "^2.1.2",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-qunit": "^5.1.5",
    "ember-resolver": "^8.0.3",
    "ember-source": "~4.1.0",
    "ember-source-channel-url": "^3.0.0",
    "ember-template-lint": "^3.15.0",
    "ember-try": "^1.4.0",
    "eslint": "^7.32.0",
    "eslint-plugin-ember": "^10.5.8",
    "eslint-plugin-node": "^11.1.0",
    "license-checker": "^25.0.1",
    "loader.js": "^4.7.0",
    "npm-run-all": "^4.1.5",
    "qunit-dom": "^1.6.0"
  },
  "engines": {
    "node": "10.* || >= 12"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  }
}
